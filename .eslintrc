{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "airbnb",
    "rules": {
        "comma-dangle": [1, "never"],
        "indent": [2, 4, {"SwitchCase": 1}],
        "max-len": [2, 120, 4, {"ignoreTrailingComments": true, "ignoreUrls": true}],
        "prefer-template": 0,
        "new-cap": [2, {"capIsNewExceptions": ["DateTimeFormat"]}],
        "object-curly-spacing": [1, "never"],
        "react/jsx-indent": [1, 4],
        "react/jsx-indent-props": [1, 4]
    }
}
